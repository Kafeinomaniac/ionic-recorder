    "test": "find ./app -type f | xargs sed -i \"s/[ \t]*$//\" ; bin/gulp --gulpfile test/gulpfile.ts --cwd ./ test",
    "karma": "bin/gulp --gulpfile test/gulpfile.ts --cwd ./ test.karma.debug",
    "build": "bin/gulp --gulpfile test/gulpfile.ts --cwd ./ ionic.build"
