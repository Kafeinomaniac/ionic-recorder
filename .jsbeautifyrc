{
    "eol": "\n",
    "indent_with_tabs": false,
    "indent_char": " ",
    "indent_size": 4,
    "brace_style": "end-expand,preserve-inline",

    "space_in_empty_paren": false,
    "space_in_paren": false,
    "end_with_newline": true,
    "css": {
        "indent_size": 4
    },

    "beautify.language": {
        "js": {
            "type": ["javascript", "json"],
            "filename": [".jshintrc", ".jsbeautify"],
            "ext": ["js", "json"]
        },
        "css": ["css", "scss"],
        "html": ["htm", "html"]
    }

}
